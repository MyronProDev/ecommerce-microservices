{
  "name": "ecommerce/shared-bundle",
  "description": "Shared bundle for Product and Order microservices",
  "type": "symfony-bundle",
  "license": "MIT",
  "version": "1.0",
  "autoload": {
    "psr-4": {
      "Ecommerce\\SharedBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ecommerce\\SharedBundle\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.1",
    "symfony/framework-bundle": "^6.4",
    "symfony/validator": "^6.4",
    "symfony/uid": "^6.4",
    "doctrine/orm": "^3.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.5",
    "symfony/phpunit-bridge": "^6.4"
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
